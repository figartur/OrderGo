<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.OrderGo" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/themebeige</item>
        <item name="colorPrimaryVariant">@color/darkgreen</item>
        <item name="colorOnPrimary">@color/mossgreen</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/darkgreen</item>
        <item name="colorSecondaryVariant">@color/themeblack</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
        <item name="android:textColor">?attr/colorSecondaryVariant</item>
    </style>
</resources>